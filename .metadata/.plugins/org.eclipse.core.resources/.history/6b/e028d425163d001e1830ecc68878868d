package com.kodnest.loops;

public class loop_11 {

	public static void main(String[] args) {
		do

		{

		System.out.println("Hello");

		for(int i=0;i<=100;i=i+4)

		{

		System.out.println("Kodnest");

		if(i==19)

		{

		break;

		}

		}

		System.out.println("Technologies");

		break;

		}while(true);
	}

}
